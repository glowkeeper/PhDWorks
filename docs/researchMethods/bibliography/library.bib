@article{fowler_targeting_2010,
  title = {Targeting the Endocannabinoid System for the Treatment of Cancer--a Practical View},
  volume = {10},
  issn = {1873-4294},
  abstract = {In recent years, considerable interest has been generated by findings that cannabinoids not only have useful palliative effects, but also can affect the viability and invasivity of a variety of different cancer cells. In the present review, the potential of targeting the cannabinoid system for the treatment of cancer is considered from a practical, rather than a mechanistic viewpoint, addressing questions such as whether human tumour cells express CB receptors; whether the potencies of action of cannabinoids in vitro match the potencies expected on the base of receptor theory; what is known about the in vivo effects of cannabinoids and cancer, and how relevant the experiments undertaken are to the clinical situation; and finally, what approaches can be taken to minimise unwanted effects of cannabinoid treatment. It is concluded that cannabinoids (or agents modulating the endogenous cannabinoid system) are an attractive target for drug development in the cancer area, but that more in vivo studies, particularly those investigating the potential of cannabinoids as an addition to current treatment strategies, are needed.},
  timestamp = {2016-05-25T09:15:39Z},
  number = {8},
  journal = {Current topics in medicinal chemistry},
  author = {Fowler, Christopher J and Gustafsson, Sofia B and Chung, Sui Chu and Persson, Emma and Jacobsson, Stig O P and Bergh, Anders},
